{
    "name": "wucdbm/filter-bundle",
    "type": "symfony-bundle",
    "description": "A Bundle that takes away some of the pain developing filtered lists of things",
    "keywords": ["wucdbm", "filter", "filter bundle", "symfony"],
    "authors": [
        {
            "name": "Martin Kirilov",
            "email": "wucdbm@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "symfony/http-foundation": "~8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3",
        "symfony/config": "~8.0",
        "symfony/dependency-injection": "~8.0",
        "symfony/http-kernel": "~8.0",
        "symfony/form": "~8.0",
        "symfony/options-resolver": "~8.0",
        "symfony/validator": "~8.0",
        "doctrine/orm": "~3"
    },
    "suggest": {
        "symfony/framework-bundle": "~8.0",
        "symfony/config": "~8.0",
        "symfony/dependency-injection": "~8.0",
        "symfony/http-kernel": "~8.0",
        "symfony/form": "~8.0",
        "symfony/options-resolver": "~8.0",
        "symfony/validator": "~8.0",
        "doctrine/orm": "~3"
    },
    "autoload": {
        "psr-4": { "Wucdbm\\Bundle\\WucdbmFilterBundle\\": "src" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "license": "MIT"
}
